{% trans_default_domain 'FOSUserBundle' %}

{% form_theme form 'VanWallBundle:Form:form_widget_simple.html.twig' 'VanWallBundle:Form:fields.html.twig' 'VanWallBundle:Form:textarea_widget.html.twig' %} 

<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register col-md-4 col-md-offset-1">
    <h2>{{ 'registration.submit'|trans }}</h2>
    {{ form_widget(form) }}
    <div>
        <input type="submit" value="{{ 'registration.submit'|trans }}" class="btn btn-primary" />
    </div>
</form>